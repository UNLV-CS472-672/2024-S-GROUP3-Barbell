{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug in Exponent",
      "request": "launch",
      "type": "reactnative",
      "cwd": "${workspaceFolder}",
      "platform": "exponent",
      "expoHostType": "local"
    },
    {
      "name": "Next.js",
      "type": "node-terminal",
      "request": "launch",
      "command": "pnpm dev",
      "cwd": "${workspaceFolder}/apps/nextjs/",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
